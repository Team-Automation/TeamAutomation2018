<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Gmail Automation Script " parallel="false">

    <parameter name="OS" value="macOS Mojave"/>
    <parameter name="browser" value="chrome"/>
    <parameter name="browserVersion" value="64.0"/>
    <parameter name="url" value="https://www.gmail.com"/>

    <test name = "Gmail Home">
        <classes>
            <class name="GmailLogin.GmailTestLogin"/>
        </classes>
    </test>

</suite>